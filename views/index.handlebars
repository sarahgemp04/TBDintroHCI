<!doctype html>

<html>
<head>
	<title>Total Body Development</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">
<body>
<div class="objects">
  <h1>Welcome, @username!</h1> 
  <form action="myworkout">
  	<button class="btn btn-1 btn-1a">
    	My Workout
    	<img src="/img/weightlifting.png" class="icon"></img>
  	</button>
  </form>
  <form action="dailylog">
  	<button class="btn btn-1 btn-1a">
  		Daily Log
    	<img src="img/calendar.png" class="icon"></img>
  	</button>
  </form>
  <form action="favorites">
  	<button class="btn btn-1 btn-1a">
    	Favorites
    	<img src="img/star.png" class="icon"></img>
  	</button>
  </form>
  <form action="notes">
  	<button class="btn btn-1 btn-1a">
    	Notes
    	<img src="/img/notepad.png" class="icon"></img>
  	</button>
  </form>
</div>

<script src="https://code.jquery.com/jquery.js"></script>
<script src="/js/bootstrap.min.js"></script>
</body>
</html>
