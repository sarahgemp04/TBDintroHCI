  	<div class="container">
      <h1> My List </h1>  

      {{#if dropdown}}
      
      {{#each workout}}
      <div class="dropElement">
          <a href=# style="width: 50%; font-size: 25px; padding: 0px;" class="dropTitle btnd btn-d btn-drop">{{name}} Workout</a>

          <div style="display: none;" class="drop-box" id={{video}}>
           
              <h4 style="text-align: center">Your Video:</h4> 
              <iframe style="width: 100%; height: 250px; " src={{video}} frameborder="0" allowfullscreen></iframe></br>
              <!--
              {{Description}}
              <a href={{info}} style="color: #00BCD4;">Learn more and get some fitspiration here!</a>-->
          </div>
      </div>      
      {{/each}}
      
      
    {{else}}


      {{#each workout}}
      <div class="videoContainer" style="text-align:center">
        <h2>{{name}} {{type}} workout</h2>
        <iframe width="560" height="315" src="{{video}}" frameborder="0" allowfullscreen></iframe>
      </div>
      {{/each}}
      {{/if}}
      <a href="notes"> <h3> Add notes </h3> </a>
